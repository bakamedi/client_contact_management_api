{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS Debug",
        "runtimeExecutable": "yarn",
        "runtimeArgs": [
          "run",
          "start:dev"
        ],
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        "smartStep": true,
        "internalConsoleOptions": "openOnSessionStart",
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }
  